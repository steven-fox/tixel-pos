parameters:
    level: 5
    paths:
        - app
        - bootstrap
        - config
        - database
        - routes
        - tests
    databaseMigrationsPath:
        - database/migrations
    tmpDir: build/phpstan
    checkOctaneCompatibility: true
    checkModelProperties: true
    checkMissingIterableValueType: true
    noEnvCallsOutsideOfConfig: true
    treatPhpDocTypesAsCertain: false
